{
  "name": "bssniper",
  "version": "2.1.0",
  "description": "A collection of useful functions for building Beat Saber playlists based on player scores.",
  "main": "build/main.js",
  "types": "build/main.d.ts",
  "private": true,
  "files": [
    "./build"
  ],
  "scripts": {
    "test": "cd src && ts-node test.ts",
    "build": "run-script-os",
    "build:nix": "rimraf ./build && tsc && cp -r src/resources build/resources",
    "build:win32": "rimraf ./build && tsc && xcopy src\\resources build\\resources /e /i /h /y",
    "prepublish": "npm run build && rimraf ./build/test.* && rimraf ./build/resources/*.psd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocker1904/BSSniper.git"
  },
  "author": "Rocker1904",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rocker1904/BSSniper/issues"
  },
  "homepage": "https://github.com/rocker1904/BSSniper#readme",
  "dependencies": {
    "axios": "^0.21.2",
    "axios-retry": "^3.1.9",
    "csv-parser": "^2.3.5",
    "jquery-csv": "^1.0.11",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-google": "^0.14.0",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6"
  }
}
